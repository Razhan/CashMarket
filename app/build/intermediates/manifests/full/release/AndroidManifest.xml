<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.line_chart"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application android:theme="@android:style/Theme.NoTitleBar" >
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.over1.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.over1.ButtonDemoActivity"
            android:label="@string/title_activity_button_demo"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" >
        </activity>
        <activity
            android:name="com.example.over1.XYChartBuilder"
            android:label="@string/title_activity_xychart_builder"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name="com.example.over1.FileChooser"
            android:label="@string/title_activity_file_chooser" >
        </activity>
        <activity
            android:name="com.example.over1.Help"
            android:label="@string/title_activity_help" >
        </activity>
        <activity
            android:name="com.example.over1.Rules_webview"
            android:label="@string/title_activity_rules_webview" >
        </activity>
        <activity
            android:name="com.example.over1.Test"
            android:label="@string/title_activity_test" >
        </activity>
    </application>

</manifest>